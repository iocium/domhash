{
  "name": "@iocium/domhash",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "Structure- and layout-aware perceptual hashing for HTML/DOM trees.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/umd/index.umd.js",
  "unpkg": "dist/umd/index.umd.js",
  "jsdelivr": "dist/umd/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "browser": "./dist/browser/index.mjs"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup && npm run build:umd",
    "build:umd": "rollup -c rollup.config.js",
    "test": "jest --runInBand --no-cache",
    "test:coverage": "jest --runInBand --coverage --no-cache",
    "docs": "typedoc"
  },
  "bin": {
    "domhash": "dist/cli.js"
  },
  "keywords": [
    "hash",
    "dom",
    "layout",
    "html",
    "similarity",
    "perceptual"
  ],
  "author": "iocium",
  "license": "MIT",
  "dependencies": {
    "@noble/hashes": "^1.3.0",
    "commander": "^14.0.0",
    "diff": "^8.0.2",
    "linkedom": "^0.18.11",
    "typedoc": "^0.28.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/bun": "^1.2.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "rollup": "^4.44.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
